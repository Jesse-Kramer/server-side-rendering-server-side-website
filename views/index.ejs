<%- include('./partials/head') %>

    <main>
        <section>
            <h2>Categories</h2>
            <% categories.forEach(category=> { %>
                <h3>
                    <%= category.name %>
                </h3>
                <% }); %>
        </section>

        <section>
            <h2>Posts</h2>
            <% posts.forEach(post=> { %>
                <h3>
                    <%= post.title.rendered %>
                </h3>
                <p>
                    <%= post.content.rendered %>
                </p>
                <% }); %>
        </section>

        <section>
            <h2>Users</h2>
            <% users.forEach(user=> { %>
                <h3>
                    <%= user.name %>
                </h3>
                <p>
                    <%= user.description %>
                </p>
                <% }); %>
        </section>
    </main>

    <%- include('./partials/foot') %>