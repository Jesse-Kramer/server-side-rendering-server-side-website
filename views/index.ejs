<%- include('./partials/head') %>

<main>
    <h2>Posts</h2>
    <% posts.forEach(post => { %>
        <h3><%= post.title.rendered %></h3>
        <p><%= post.content.rendered %></p>
    <% }); %>

    <h2>Users</h2>
    <% users.forEach(user => { %>
        <h3><%= user.name %></h3>
        <p><%= user.description %></p>
    <% }); %>
</main>

<%- include('./partials/foot') %>